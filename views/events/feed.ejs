        <ul>
            <li><a href="/events/feed" data-icon="home" class="ui-btn-active ui-state-persist">Feed</a></li>
            <li><a href="/events/new" data-icon="Plus">New Event</a></li>
        </ul>
    </div><!-- /navbar -->
  </div><!-- /footer -->

